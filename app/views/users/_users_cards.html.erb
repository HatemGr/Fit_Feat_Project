<% users.order(id:'asc').each do |user|%>
<div class="user-card">

  <div class="user-name">
    <h1> 
      <%= user.first_name%> <%= user.last_name%>
    </h1>
  </div>

  <div class="user-data">
    <ul>
      <li>
        Adresse :<%= user.address%>
      </li>
      <li>
        Longitude: <%= user.longitude%> Latitude: <%= user.latitude%>
      </li>
      <li>
        Ville: <%= user.city.name%>
      </li>
    </ul>
  </div>

  <div class="user-links">
    <%= link_to 'User Show', user_path(user) %>
    <%= link_to 'User Edit', edit_user_path(user) %>
  </div>

</div>
<%end%>